package com.demo.test;

import com.demo.arrays.My2dArray;

public class Test2DArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		My2dArray ob=new My2dArray();
		My2dArray ob1=new My2dArray();

		ob.acceptArr();
		ob.displayArr();
		
		int[] rowwise=ob.sumRowWise();
		System.out.println(rowwise[0]);
		System.out.println(rowwise[1]);
		System.out.println(rowwise[2]);
		
		
		private static void showarray(int[] rowwise) {
			for(int i=0;i<rowwise.length;i++) {
				System.out.println(rowwise[i]);
			}
		}
	}

}
